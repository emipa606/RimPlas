  <!-- ===================== RimRayon =========================== -->
  <ThingDef ParentName="ResourceBase">
    <defName>RimRayon</defName>
    <label>RimRayon</label>
    <description>An early semisynthetic fibre. Made from cellulose (wood) and an organic base (chemfuel).</description>
    <graphicData>
      <texPath>Things/Item/RimPlas_RimRayon</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <MaxHitPoints>120</MaxHitPoints>
      <MarketValue>2.1</MarketValue>
      <Mass>0.032</Mass>
      <Flammability>0.9</Flammability>
      <DeteriorationRate>3</DeteriorationRate>
      <StuffPower_Armor_Sharp>0.60</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>0.18</StuffPower_Armor_Blunt>
      <StuffPower_Armor_Heat>0.30</StuffPower_Armor_Heat>
      <StuffPower_Insulation_Cold>20</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>20</StuffPower_Insulation_Heat>
    </statBases>
    <stuffProps>
      <categories>
        <li>Fabric</li>
      </categories>
      <color>(255,224,187)</color>
      <statFactors>
        <Flammability>0.9</Flammability>
        <MaxHitPoints>1.2</MaxHitPoints>
      </statFactors>
      <commonality>0.18</commonality>
    </stuffProps>
    <thingCategories>
      <li>Textiles</li>
    </thingCategories>
    <comps>
      <li>
        <compClass>CompColorable</compClass>
      </li>
    </comps>
    <techLevel>Industrial</techLevel>
    <tradeTags>
      <li>Exotic</li>
    </tradeTags>
    <burnableByRecipe>true</burnableByRecipe>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
  </ThingDef>
