<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- ============ CarboPitch ============================== -->
  <RecipeDef>
    <defName>MakeCarboPitch</defName>
    <jobString>Making Pitch</jobString>
    <label>Make Pitch</label>
    <description>Make 100 Pitch</description>
    <workAmount>700</workAmount>
    <workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
    <effectWorking>Cremate</effectWorking>
    <soundWorking>Recipe_Cremate</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <targetCountAdjustment>100</targetCountAdjustment>
    <workSkill>Intellectual</workSkill>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </filter>
        <count>100</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Synthbase</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
    </ingredients>
    <products>
      <CarboPitch>100</CarboPitch>
    </products>
    <fixedIngredientFilter>
      <thingDefs>
        <li>WoodLog</li>
        <li>Synthbase</li>
      </thingDefs>
    </fixedIngredientFilter>
    <recipeUsers>
      <li>BiofuelRefinery</li>
    </recipeUsers>
    <researchPrerequisite>RimPlas_CarboSynth</researchPrerequisite>
    <skillRequirements>
      <Crafting>4</Crafting>
      <Intellectual>5</Intellectual>
    </skillRequirements>
  </RecipeDef>
